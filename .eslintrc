{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "eslint-config-airbnb"
  ],
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "func-names": "off",
    "indent": [
      "error",
      2
    ],
    "linebreak-style": "off",
    "max-len": [
      "error",
      120
    ],
    "no-console": "off",
    "no-new": "off",
    "no-plusplus": "off",
    "no-restricted-syntax": "off",
    "prefer-arrow-callback": "off",
    "prefer-destructuring": "off",
    "prefer-template": "off",
    "quotes": [
      "error",
      "double"
    ],
    "radix": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "vars-on-top": "off"
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}
